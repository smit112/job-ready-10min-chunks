# Environment configuration for Agentic AI Configuration Research System

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Database Configuration
DATABASE_URL=sqlite:///./config_research.db
REDIS_URL=redis://localhost:6379

# File Paths
DATA_DIR=/workspace/data
CONFIGS_DIR=/workspace/configs
AGENTS_DIR=/workspace/agents
UTILS_DIR=/workspace/utils

# Excel Processing
EXCEL_TEMPLATES_DIR=/workspace/data/excel_templates
EXCEL_OUTPUT_DIR=/workspace/data/excel_output

# PDF Processing
PDF_DOCS_DIR=/workspace/data/pdf_docs
PDF_ERRORS_DIR=/workspace/data/pdf_errors

# Link Analysis
LINK_CACHE_DIR=/workspace/data/link_cache
MAX_LINK_DEPTH=3
LINK_TIMEOUT=30

# AI Model Settings
DEFAULT_MODEL=gpt-4
MAX_TOKENS=4000
TEMPERATURE=0.1

# Validation Settings
VALIDATION_TIMEOUT=60
MAX_VALIDATION_ATTEMPTS=3

# Dashboard Settings
DASHBOARD_HOST=0.0.0.0
DASHBOARD_PORT=8000
DEBUG=false

# Logging
LOG_LEVEL=INFO
LOG_FILE=/workspace/logs/app.log